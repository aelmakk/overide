#include <stdio.h>
#include <stdlib.h>

int     main(void)
{
    int something;
    char buff[];

    something = 0;
    fgets(buff, 100, stdin); // char *fgets(char *s, int size, FILE *stream);
    strlen(buff);
    tmp = 0;
    while (tmp < strlen(buff))
    {
        if ((buff[tmp] <= 0x40) || (buff[tmp] > 0x5a) ) // 0x40 = 64 @
            tmp ++;
        else
            buff[tmp] = buff[tmp] ^ 32;
    }
    printf(buff);
    exit();
}



